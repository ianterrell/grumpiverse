= form_for [:admin, @character], :html => { :multipart => true } do |f|
  = render :partial => "/admin/characters/form", :locals => { :f => f }
  = f.submit
  = link_to "Cancel", admin_character_path(@character)

- content_for :sidebar do
  = link_to "Back", admin_character_path(@character), :class => "back"

